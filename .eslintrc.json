{
  "env":{"es6": true, "jest": true, "node": true},
  "rules": {
    "brace-style":["error", "1tbs"],
    "curly": ["error", "multi"],
    "quotes":["error", "double"],
    "semi":["error", "always"],
    "no-undefined":"error",
    "no-use-before-define": "error",
    "arrow-parens": ["error", "always"],
    "arrow-spacing": ["error", { "before": true, "after": true }],
    "no-undef": ["error", { "typeof": true }]
  },
  "settings": {
  },
  "parser": "babel-eslint"
}